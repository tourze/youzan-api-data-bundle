{
  "name": "tourze/youzan-api-data-bundle",
  "description": "有赞数据接口模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/console": "^6.4",
    "symfony/http-kernel": "^6.4",
    "tourze/bundle-dependency": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "youzanyun/open-sdk": "^2.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "YouzanApiDataBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YouzanApiDataBundle\\Tests\\": "tests/"
    }
  }
}