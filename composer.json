{
  "name": "tourze/youzan-api-data-bundle",
  "description": "有赞数据接口模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^3.0",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/http-kernel": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "YouzanApiDataBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload-dev": {
    "psr-4": {
      "YouzanApiDataBundle\\Tests\\": "tests/"
    }
  }
}